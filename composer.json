{
    "name": "egs33/laravel-datastore-auth",
    "authors": [
        {
            "name": "egs33",
            "email": "egs33.bicycle@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.5.0",
        "google/cloud-datastore": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "DatastoreAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DatastoreAuth\\DatastoreAuthServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true,
    "require-dev": {
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^7.3",
        "orchestra/testbench": "^3.5",
        "guzzlehttp/guzzle": ">=6.3.0",
        "google/cloud-datastore": "^1.5.6"
    }
}
